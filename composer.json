{
    "name": "genesis-global/salesforce-bundle",
    "description": "Symfony bundle that provides OAuth2 authentication and exposes Salesforce REST API methods in OOP friendly way. ",
    "type": "library",
    "keywords": [
        "salesforce"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bge-webservices/salesforce"
        }
    ],
    "require": {
        "symfony/http-kernel": "^3.4",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "genesis-global/salesforce": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6"
    },
    "autoload": {
        "psr-4": {
            "GenesisGlobal\\Salesforce\\SalesforceBundle\\": "src/"
        }
    }
}